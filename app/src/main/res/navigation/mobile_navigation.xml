<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mapr.credibanco.view.fragments.AuthorizationFragment"
        android:label="@string/title_auth"
        tools:layout="@layout/fragment_authorization" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.mapr.credibanco.view.fragments.SearchListFragment"
        android:label="@string/title_search_list"
        tools:layout="@layout/fragment_search_list" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.mapr.credibanco.view.fragments.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>